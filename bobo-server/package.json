{
    "name": "bobo-server",
    "version": "1.0.0",
    "description": "This is bobo server",
    "author": "yunseok",
    "license": "MIT",
    "scripts": {
        "start": "npm run tsc && npm run tslint && npm run serve",
        "build": "npm run tsc && npm run tslint",
        "serve": "node dist/server.js",
        "test": "NODE_ENV=test ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles",
        "watch": "NODE_ENV=test ./node_modules/jest/bin/jest.js --watchAll --runInBand --detectOpenHandles --verbose",
        "tsc": "./node_modules/typescript/bin/tsc",
        "tslint": "./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "engines": {
        "node": "10.15.3"
    },
    "dependencies": {
        "@cosmostation/cosmosjs": "^0.2.3",
        "@types/moment-timezone": "^0.5.12",
        "axios": "^0.21.1",
        "bcrypt-nodejs": "0.0.3",
        "bluebird": "3.5.3",
        "body-parser": "1.18.3",
        "compression": "1.7.3",
        "connect-mongo": "2.0.3",
        "cors": "2.8.5",
        "dotenv": "7.0.0",
        "express": "4.16.4",
        "express-session": "1.15.6",
        "express-validator": "5.3.1",
        "flightplan": "^0.6.20",
        "global": "^4.3.2",
        "helmet": "3.16.0",
        "lodash": "^4.17.15",
        "moment-timezone": "0.5.23",
        "mongoose": "5.4.19",
        "mongoose-auto-increment-reworked": "1.2.1",
        "mysql2": "1.6.5",
        "passport": "0.4.0",
        "passport-local": "1.0.0",
        "request": "2.88.0",
        "secp256k1": "^3.7.1",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@types/async": "2.4.1",
        "@types/bcrypt-nodejs": "0.0.30",
        "@types/bluebird": "3.5.26",
        "@types/body-parser": "1.17.0",
        "@types/compression": "0.0.36",
        "@types/cors": "2.8.4",
        "@types/dotenv": "6.1.0",
        "@types/express": "4.16.1",
        "@types/express-session": "1.15.12",
        "@types/helmet": "0.0.43",
        "@types/jest": "24.0.11",
        "@types/lodash": "4.14.123",
        "@types/mongodb": "3.1.22",
        "@types/mongoose": "5.3.22",
        "@types/mysql2": "github:types/mysql2",
        "@types/passport": "1.0.0",
        "@types/passport-local": "1.0.33",
        "@types/request": "2.48.1",
        "@types/sequelize": "4.27.46",
        "@types/supertest": "2.0.7",
        "jest": "24.5.0",
        "supertest": "3.4.2",
        "ts-jest": "^24.0.0",
        "ts-node": "8.0.3",
        "tslint": "5.14.0",
        "typescript": "3.3.3333"
    }
}
